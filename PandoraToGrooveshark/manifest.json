{
  "name": "Pandora to Grooveshark",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Add music from Pandora to your Grooveshark collection. ",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts" : [{
        "matches" : [ "*://www.pandora.com/*"],
        "js" : ["script.js"]
    }]
}